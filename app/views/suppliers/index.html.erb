<h1>Suppliers</h1>

<%= link_to "New Supplier", new_supplier_path %>

<table>
    <tr>
        <th> Name </th>
        <th> Website </th>
    </tr>
    <% @suppliers.each do |supplier| %>
    <tr>
        <td> <%= link_to supplier.display_name, supplier_path(supplier.id) %> </td>
        <td> <%= supplier.website %> </td>
    </tr>
    <% end %>
</table>